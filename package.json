{
	"name": "obsidian-plugin-copy-section",
	"version": "1.2.4",
	"description": "Obsidian.md plugin adding a Copy button to the top of Headed sections",
	"main": "main.js",
	"scripts": {
		"watch": "node esbuild.config.mjs --watch",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs",
		"format": "prettier 'src/**/*.ts' --write",
		"test": "jest",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"repository": {
		"url": "https://github.com/skztr/obsidian-plugin-section-copy"
	},
	"keywords": [
		"obsidian",
		"obsidian-md",
		"obsidian-plugin"
	],
	"author": "skztr <skizotoro@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.28.5",
		"@babel/preset-env": "^7.28.5",
		"@babel/preset-typescript": "^7.28.5",
		"@codemirror/language": "^6.12.1",
		"@codemirror/view": "^6.38.6",
		"@types/jest": "^30.0.0",
		"@types/node": "^25.0.3",
		"@typescript-eslint/eslint-plugin": "^8.52.0",
		"@typescript-eslint/parser": "^8.52.0",
		"babel-jest": "^30.2.0",
		"builtin-modules": "5.0.0",
		"esbuild": "^0.27.2",
		"jest": "^30.2.0",
		"obsidian": "^1.11.4",
		"prettier": "^3.7.4",
		"remark": "15.0.1",
		"ts-jest": "^29.4.6",
		"ts-node": "^10.9.2",
		"tslib": "2.8.1",
		"typescript": "5.9.3"
	}
}
