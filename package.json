{
	"name": "obsidian-plugin-copy-section",
	"version": "1.0.2",
	"description": "Obsidian.md plugin adding a Copy button to the top of Headed sections",
	"main": "main.js",
	"scripts": {
		"watch": "node esbuild.config.mjs --watch",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs",
		"format": "prettier 'src/**/*.ts' --write",
		"test": "jest",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"repository": {
		"url": "https://github.com/skztr/obsidian-plugin-section-copy"
	},
	"keywords": [
		"obsidian",
		"obsidian-md",
		"obsidian-plugin"
	],
	"author": "skztr <skizotoro@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"@codemirror/language": "^6.10.2",
		"@codemirror/view": "^6.33.0",
		"@types/jest": "^29.5.12",
		"@types/node": "^22.5.4",
		"@typescript-eslint/eslint-plugin": "8.5.0",
		"@typescript-eslint/parser": "8.5.0",
		"builtin-modules": "4.0.0",
		"esbuild": "0.23.1",
		"jest": "^29.7.0",
		"obsidian": "latest",
		"prettier": "^3.3.3",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"tslib": "2.7.0",
		"typescript": "5.6.2"
	}
}
